<%- include("templates/header") %>
<section style="background-color: #161513; color: darkgray;">
<div class="d-flex gap-2 justify-content-center py-5">
  <form action="/sendemail" method="post" class="text-center">
    Please enter email to receive a link to reset your password
    <div class="form-floating">
      <input
        name="email"
        id="floatingInput"
        class="form-control"
        type="text"
        placeholder="email"
      />
      <label for="floatingInput">Email address</label>
    </div>
    <button class="w-100 btn btn-lg btn-primary">Submit</button>
  </form>
</div>
</section>
<%- include("templates/footer") %>
